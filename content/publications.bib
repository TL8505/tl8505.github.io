@article{hillig_petitionen_2007,
	title = {Petitionen an die {Komintern} aus {Palästina} (1926/27). {Der} „{Gdud} {Avoda}“ ersucht um {Unterstützung}},
	volume = {55},
	issn = {0021-4019},
	url = {https://www.jstor.org/stable/41051808},
	abstract = {This article describes the approach of the Zionist organisation “Gedud ha-avoda” (Legion of Labour) to the Comintern. The Gedud, founded in 1920, first and foremost by immigrants from Russia, played a significant role in the history of the kibbutz movement. Its members worked on various construction sites and agricultural settlements across the mandated territory of Palestine and led their lives in accordance with socialist principles. In 1926 and 1927, the Gedud leadership turned to the Eastern Secretariat of the Comintern's Executive Commitee in order to obtain ideological and material support in their fight for a socialist Jewish Palestine. The study presented here outlines the attempts and ultimate failure of this approach.},
	number = {1},
	urldate = {2021-07-26},
