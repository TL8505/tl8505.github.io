@article{locker-biletzki_war_2016,
	title = {War and {Memory}: {The} {Israeli} {Communist} {Commemoration} of the {Spanish} {Civil} {War}, 1936-1986},
	volume = {31},
	issn = {2159-0370},
	shorttitle = {War and {Memory}},
	url = {https://www.jstor.org/stable/44631126},
	abstract = {The Spanish Civil War of 1936-1939 aroused strong responses in the Jewish Yishuv in Palestine. The support for the Spanish Republic—prevalent in the Zionist left as well as among the Communists—resulted in young Jews and Arabs volunteering to fight in Spain. These volunteers, primarily Jewish Communists, became part of a cult created around the war by the Communist Party. This article will examine the content of this cult while relating it to parallel groups in the West and in East Germany. Through this analysis, the ideological elements, heroes, modes of memory, and dissemination of the memory of the war will be explored.},
	number = {2},
	urldate = {2021-07-01},
