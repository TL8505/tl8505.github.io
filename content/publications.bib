}

@article{shapira_rethinking_2016,
	title = {Rethinking reverence for {Stalinism} in the kibbutz movement},
	volume = {22},
	issn = {1353-7121},
	url = {https://doi.org/10.1080/13537121.2015.1111640},
	doi = {10.1080/13537121.2015.1111640},
	abstract = {The reverence for Stalinism by the main kibbutz movements, which through revolutionary rhetoric helped perpetuate leaders who had reached the dysfunctional phase, was wrongly interpreted. Historians missed leaders’ efforts to induce reverence and its use for their domination, while social researchers missed its enhancement of oligarchic rule. These are explained by the suppression of critics at the hands of a co-opted functionalist scientific coalition, by ethnographers missing the impact of inter-kibbutz organizations, and by the differentiation of disciplines. Multiple ethnographies of kibbutzim (pl. of kibbutz) and inter-kibbutz organizations and the integration of various findings by a good theory exposed these failures. They point to the required integration of disciplines and to the need for reform of scientific publication decision-making aimed at preventing such failures.},
	number = {1},
	urldate = {2021-07-01},
	journal = {Israel Affairs},
	author = {Shapira, Reuven},
	month = jan,
	year = {2016},
	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/13537121.2015.1111640},
	keywords = {co-opted functionalist researchers, dominant scientific coalition, dysfunctional leaders, Inter-kibbutz organizations, oligarchy, reverence for Stalinism},
	pages = {20--44},
	file = {Full Text PDF:files/386/Shapira - 2016 - Rethinking reverence for Stalinism in the kibbutz .pdf:application/pdf;Snapshot:files/387/13537121.2015.html:text/html},
}

